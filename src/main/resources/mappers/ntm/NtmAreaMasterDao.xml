<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lemonico.ntm.dao.NtmAreaMasterDao">

    <select id="getNtmAreaMasterByCode" resultType="com.lemonico.common.bean.Mc108_ntm_area_master">
        select area_code, area_name
        from mc108_ntm_area_master mc108
        where mc108.area_code = #{code}
    </select>

</mapper>